{
  "name": "linkorb/wiki-bundle",
  "description": "Symfony 6|7 project Wiki Bundle",
  "homepage": "https://github.com/linkorb/wiki-bundle",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packagist.com/linkorb"
    },
    {
      "packagist.org": false
    }
  ],
  "keywords": [
    "form",
    "linkorb"
  ],
  "type": "symfony-bundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "LinkORB Engineering",
      "email": "engineering@linkorb.com",
      "role": "Development"
    },
    {
      "name": "Kaushik Prajapati",
      "email": "prajapatikaushik@gmail.com",
      "role": "Developer"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "LinkORB\\Bundle\\WikiBundle\\": "src/"
    }
  },
  "require": {
    "andrewfenn/pid-helper": "^0.1.0",
    "czproject/git-php": "^4.3",
    "doctrine/doctrine-bundle": "^2.5",
    "league/commonmark": "^2.0",
    "symfony/expression-language": "*",
    "symfony/form": "*",
    "symfony/framework-bundle": "^6.4.0 || ^7.2.0",
    "symfony/orm-pack": "*",
    "symfony/security-bundle": "*",
    "symfony/validator": "*",
    "symfony/yaml": "*",
    "twig/markdown-extra": "^3.3"
  }
}
